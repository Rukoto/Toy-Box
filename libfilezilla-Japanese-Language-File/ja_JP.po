#: ../../../source/libfilezilla/lib/socket_errors.cpp:54
msgid "Permission denied"
msgstr "アクセス拒否"

#: ../../../source/libfilezilla/lib/socket_errors.cpp:50
msgid "Process file table overflow"
msgstr "プロセス ファイル テーブルのオーバーフロー"

